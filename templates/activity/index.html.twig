{% extends 'backoffice.html.twig' %}

{% block titre %}
  {{ title }}
{% endblock %}
{% block title %}
  {{ parent() }} {{ title }}
{% endblock %}


{% block body %}
  {% block stylesheet %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/select2.css') }}">
  {% endblock %}



  {{ form(form) }}


  {% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/jquery.min.js') }}"></script>
    <script src="{{ asset('js/select2.js') }}"></script>
    <script>
      $(function(){
        $('.select2').select2()

      })
    </script>
  {% endblock %}
{% endblock %}
